<Page x:Class="PH4_WPF.Browser.Page_Hash"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:PH4_WPF.Browser"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Page_Hash" Background="#FF55C595">

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="26*"/>
            <RowDefinition Height="199*"/>
        </Grid.RowDefinitions>
        <Border BorderThickness="1" Grid.Row="1" Grid.ColumnSpan="1" CornerRadius="30,30,0,0" BorderBrush="White" Background="White">
            <Border.Effect>
                <DropShadowEffect RenderingBias="Performance" Opacity="0.3" Direction="0" ShadowDepth="10" BlurRadius="30" Color="#FF075634" />
            </Border.Effect>
            <Rectangle HorizontalAlignment="Left" Height="96" Margin="52,198,0,0" VerticalAlignment="Top" Width="191" Fill="#FFF1F4F9"/>
        </Border>

    </Grid>
</Page>
